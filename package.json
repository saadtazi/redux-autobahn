{
  "name": "redux-autobahn",
  "version": "0.0.0",
  "description": "A redux middleware for WAMP protocol using autobahn.",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm run lint && npm test && npm run compile && git add dist",
    "compile": "babel src -d dist",
    "ci": "npm run lint && npm run coveralls",
    "cov": "nyc --reporter=html --reporter=text npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "postversion": "git push && git push --tags",
    "test": "mocha",
    "test:dev": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "http://tbd"
  },
  "keywords": [
    "redux",
    "autobahn",
    "publish/subscribe",
    "pubsub",
    "websocket",
    "wamp"
  ],
  "author": "Saad Tazi <stazi@fruitsoftware.com>",
  "license": "ISC",
  "devDependencies": {
    "autobahn": "^0.9.9",
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "husky": "^0.11.4",
    "mocha": "^2.4.5",
    "nyc": "^6.2.1",
    "redux": "^3.4.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "wamp.rt": "^0.1.4"
  },
  "peerDependencies": {
    "autobahn": "^0.9.9",
    "redux": "^3.4.0"
  },
  "nyc": {
    "include": [
      "src/**/*"
    ]
  }
}
